{
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:devclient": "expo start --dev-client --localhost --clear --port 8082",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint app App.tsx --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e:build:android": "detox build -c android.debug",
    "e2e:test:android": "detox test -c android.debug --headless"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.10.0",
    "@tanstack/react-query": "^5.54.1",
    "axios": "^1.7.7",
    "expo": "~51.0.23",
    "expo-device": "~6.0.2",
    "expo-document-picker": "~12.0.1",
    "expo-image-picker": "~15.0.6",
    "expo-linear-gradient": "~13.0.2",
    "expo-notifications": "~0.28.19",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "^0.74.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "victory-native": "^36.9.2",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli-server-api": "^20.0.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.73",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^5.0.0",
    "detox": "^20.13.5",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.5"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|victory-native|@react-native-async-storage/async-storage|@react-navigation/.*|react-native-svg|@tanstack/react-query|expo-device|expo-secure-store|expo-notifications|expo-modules-core|expo-font|expo-asset|expo-linear-gradient|@expo/vector-icons)/)"
    ],
    "moduleNameMapper": {
      "^react-native$": "<rootDir>/jest/mocks/react-native.js",
      "^.*vendor/emitter/EventEmitter$": "<rootDir>/jest/mocks/EventEmitter.js",
      "^.*NativeAnimatedHelper$": "<rootDir>/jest/mocks/NativeAnimatedHelper.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/",
      "<rootDir>/e2e/"
    ]
  }
}
